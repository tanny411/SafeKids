{
    "manifest_version":2,
    "name":"Safe Kids",
    "version":"1.0",
    "web_accessible_resources":[
        "Links/*",
        "Words/*"    
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background":{
        "scripts": [
            "background.js"
        ]
    },
    "permissions":["tabs"],
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup":"popup.html",
        "default_title":"Safe Kids - Protect your kids from the Internet"
    }
}
